{
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo",
    "skipLibCheck": true,
    "module": "ESNext",
    "target": "ES6",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "noEmit": true,
    "jsx": "react",
    "baseUrl": ".",
    "paths": {
      "#/*": ["src/*"],
      "#components/*": ["src/components/*"],
      "#assets/*": ["src/assets/*"]
    }
  },
  "include": ["src", "vite.config.ts"],
  "files": ["src/custom.d.ts"]
}
